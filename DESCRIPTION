    'str_functions.R'
    'str_is.R'
    'str_lowercase.R'
    'str_sentence_case.R'
    'str_snake_case.R'
    'str_spine_case.R'
    'str_title_case.R'
    'str_transform.R'
    'str_ucfirst.R'
    'str_uppercase.R'
    'utils.R'
    'zzz.R'
